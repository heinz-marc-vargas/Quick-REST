<h1>Listing api_pages</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th>Published on</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @api_pages.each do |api_page| %>
  <tr>
    <td><%= api_page.title %></td>
    <td><%= api_page.content %></td>
    <td><%= api_page.published_on %></td>
    <td><%= link_to 'Show', api_page %></td>
    <td><%= link_to 'Edit', edit_api_page_path(api_page) %></td>
    <td><%= link_to 'Destroy', api_page, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Page', new_api_page_path %>
